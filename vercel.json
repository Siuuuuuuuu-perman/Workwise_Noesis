{
  "version": 2,
  "builds": [
    {
      "src": "backend/app/main_production.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/app/main_production.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/index.html"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "functions": {
    "backend/app/main_production.py": {
      "maxDuration": 30
    }
  }
}
